    "# SOAL NO. 10\n",
    "import control as ct\n",
    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "\n",
    "# Parameter Sistem MISO\n",
    "\n",
    "A = np.array([[0, 1],\n",
    "              [-1, -3]])\n",
    "\n",
    "B = np.array([[0, 0],\n",
    "              [2, 4]])  \n",
    "C = np.array([[1, 0]])\n",
    "D = np.array([[0, 0]])\n",
    "\n",
    "# a. Menentukan Dua Transfer Function (H1 dan H2)\n",
    "# Input ke-1\n",
    "sys_H1 = ctrl.ss2tf(A, B[:, [0]], C, D[:, [0]])\n",
    "\n",
    "# Input ke-2\n",
    "sys_H2 = ctrl.ss2tf(A, B[:, [1]], C, D[:, [1]])\n",
    "\n",
    "print(\"\\n=== Transfer Function dari masing-masing input ===\")\n",
    "print(\"H1(s) (dari input 1 ke output):\")\n",
    "print(sys_H1)\n",
    "print(\"\\nH2(s) (dari input 2 ke output):\")\n",
    "print(sys_H2)\n",
    "\n",
    "# b. Tampilkan dan Bandingkan Step Response\n",
    "\n",
    "t = np.linspace(0, 10, 1000)\n",
    "t1, y1 = ctrl.step_response(sys_H1, T=t)\n",
    "t2, y2 = ctrl.step_response(sys_H2, T=t)\n",
    "\n",
    "plt.figure(figsize=(9, 5))\n",
    "plt.plot(t1, y1, label='Respons dari Input 1 (H1)', linewidth=2)\n",
    "plt.plot(t2, y2, label='Respons dari Input 2 (H2)', linewidth=2, linestyle='--')\n",
    "plt.title('Perbandingan Step Response dari Dua Input (MISO System)')\n",
    "plt.xlabel('Waktu (detik)')\n",
    "plt.ylabel('Respons Output y(t)')\n",
    "plt.grid(True)\n",
    "plt.legend()\n",
    "plt.show()\n",
    "\n",
    "# c. Analisis kestabilan (optional)\n",
    "poles = np.linalg.eigvals(A)\n",
    "print(\"\\nPole sistem (sama untuk semua input):\", poles)\n",
    "\n",
    "if np.all(np.real(poles) < 0):\n",
    "    print(\"Interpretasi: Sistem STABIL karena semua pole di sebelah kiri s-plane (Re < 0).\")\n",
    "else:\n",
    "    print(\"Interpretasi: Sistem TIDAK STABIL.\")"
   ]
  },
