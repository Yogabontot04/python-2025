   "source": [
    "# SOAL NO. 7\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "import control as ctrl\n",
    "\n",
    "# a. Definisikan model state-space\n",
    "A = np.array([[0, 1],\n",
    "              [-1, -3]])\n",
    "B = np.array([[0],\n",
    "              [1]])\n",
    "C = np.array([[1, 0]])\n",
    "D = np.array([[0]])\n",
    "\n",
    "# Membuat sistem state-space\n",
    "sys_ss = ctrl.ss(A, B, C, D)\n",
    "\n",
    "print(\"=== Model State-Space ===\")\n",
    "print(\"A =\\n\", A)\n",
    "print(\"B =\\n\", B)\n",
    "print(\"C =\\n\", C)\n",
    "print(\"D =\\n\", D)\n",
    "\n",
    "# a. Plot Step Response\n",
    "t, y = ctrl.step_response(sys_ss)\n",
    "\n",
    "plt.figure(figsize=(8, 5))\n",
    "plt.plot(t, y, linewidth=2)\n",
    "plt.title('Step Response Sistem State-Space')\n",
    "plt.xlabel('Waktu (detik)')\n",
    "plt.ylabel('Respons y(t)')\n",
    "plt.grid(True)\n",
    "plt.show()\n",
    "\n",
    "# b. Konversi ke bentuk Transfer Function\n",
    "sys_tf = ctrl.ss2tf(A, B, C, D)\n",
    "\n",
    "print(\"\\n=== Bentuk Transfer Function ===\")\n",
    "print(sys_tf)\n",
    "\n",
    "# Analisis Kestabilan\n",
    "poles = np.linalg.eigvals(A)\n",
    "print(\"\\nPole sistem:\", poles)\n",
    "\n",
    "if np.all(np.real(poles) < 0):\n",
    "    print(\"Interpretasi: Sistem STABIL karena semua pole berada di sebelah kiri s-plane (Re < 0).\")\n",
    "elif np.any(np.real(poles) > 0):\n",
    "    print(\"Interpretasi: Sistem TIDAK STABIL karena ada pole dengan bagian real positif.\")\n",
    "else:\n",
    "    print(\"Interpretasi: Sistem MARGINAL STABIL (pole berada pada sumbu imajiner).\")\n"
   ]
