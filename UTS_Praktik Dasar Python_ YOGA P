    "# SOAL NO. 4\n",
    "import control as ct\n",
    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "\n",
    "# --- Parameter Sistem ---\n",
    "K_values = [1, 3, 5, 10]\n",
    "\n",
    "# --- Plotting ---\n",
    "plt.figure(figsize=(12, 8))\n",
    "\n",
    "# Iterasi untuk setiap nilai K\n",
    "for K in K_values:\n",
    "    # Definisikan numerator dan denominator untuk H(s) = K / (10s + 1)\n",
    "    numerator = [K]\n",
    "    denominator = [T, 1]\n",
    "\n",
    "    # Buat transfer function\n",
    "    H = ct.tf(numerator, denominator)\n",
    "    print(f\"Transfer Function untuk K={K}:\")\n",
    "    print(H)\n",
    "\n",
    "    # Hitung step response\n",
    "    # Tentukan rentang waktu simulasi yang cukup panjang (misal 5 kali T)\n",
    "    time = np.linspace(0, 5 * T, 500)\n",
    "    time, response = ct.step_response(H, T=time)\n",
    "\n",
    "    # Plot respons\n",
    "    plt.plot(time, response, label=f'K = {K}')\n",
    "\n",
    "# --- Konfigurasi Plot ---\n",
    "plt.title('Step Response Sistem Orde Pertama dengan Variasi K (T=10s)')\n",
    "plt.xlabel('Waktu (detik)')\n",
    "plt.ylabel('Amplitudo Output')\n",
    "plt.grid(True)\n",
    "plt.legend()\n",
    "plt.show()\n",
    "\n",
    "# --- Interpretasi ---\n",
    "print(\"\\n--- Interpretasi Hasil Grafik ---\")\n",
    "print(\"Konstanta waktu (T) dari semua sistem adalah 10 detik, sehingga semua sistem akan memiliki kecepatan respons transien yang sama.\")\n",
    "print(\"Nilai gain DC (K) menentukan nilai akhir (steady-state) dari output sistem.\")\n",
    "print(\"Dari grafik, dapat dilihat bahwa:\")\n",
    "for K in K_values:\n",
    "    print(f\"- Untuk K = {K}, sistem akan stabil pada nilai akhir {K}.\")\n",
    "print(\"Secara umum: Semakin besar nilai gain (K), semakin besar nilai akhir yang dicapai oleh sistem setelah respons transien selesai.\")\n",
    "\n"
