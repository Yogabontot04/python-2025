    "# SOAL NO. 6\n",
    "import control as ct\n",
    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "\n",
    "# --- a. Membuat model State-Space (A, B, C, D) ---\n",
    "\n",
    "A = np.array([[0, 1],\n",
    "              [-1, 0]])\n",
    "\n",
    "B = np.array([[0],\n",
    "              [1]])\n",
    "\n",
    "C = np.array([[1, 0]])\n",
    "D = np.array([[0]])\n",
    "\n",
    "# Membuat objek StateSpace\n",
    "sys = ct.ss(A, B, C, D)\n",
    "\n",
    "print(\"--- Model State-Space (A, B, C, D) ---\")\n",
    "print(\"Matriks A:\")\n",
    "print(A)\n",
    "print(\"\\nMatriks B:\")\n",
    "print(B)\n",
    "print(\"\\nMatriks C:\")\n",
    "print(C)\n",
    "print(\"\\nMatriks D:\")\n",
    "print(D)\n",
    "print(\"\\nObjek Sistem StateSpace:\")\n",
    "print(sys)\n",
    "\n",
    "# --- b. Menampilkan Step Response dan Interpretasi Kestabilan ---\n",
    "\n",
    "plt.figure(figsize=(10, 6))\n",
    "\n",
    "# Hitung step response\n",
    "# Tentukan rentang waktu simulasi\n",
    "time = np.linspace(0, 10, 500)\n",
    "time, response = ct.step_response(sys, T=time)\n",
    "\n",
    "plt.plot(time, response)\n",
    "plt.title('Step Response Sistem State-Space')\n",
    "plt.xlabel('Waktu (detik)')\n",
    "plt.ylabel('Output y(t) (x1)')\n",
    "plt.grid(True)\n",
    "plt.show()\n",
    "\n",
    "# Analisis Kestabilan menggunakan Eigenvalues dari Matriks A\n",
    "eigenvalues = np.linalg.eigvals(A)\n",
    "print(\"\\n--- Interpretasi Kestabilan ---\")\n",
    "print(f\"Eigenvalues dari Matriks A (Poles Sistem): {eigenvalues}\")\n",
    "\n",
    "# Cek kestabilan\n",
    "is_stable = all(eig.real < 0 for eig in eigenvalues) or all(abs(eig.real) < 1e-9 for eig in eigenvalues) # Menangani kasus margin stabil\n",
    "\n",
    "if is_stable:\n",
    "    print(\"\\nKesimpulan Kestabilan: Sistem ini STABIL.\")\n",
    "    print(\"Alasan: Semua eigenvalues dari matriks A (yang merupakan poles sistem) berada di bidang kiri (left-half plane) dari s-plane.\")\n",
    "    print(\"Respons sistem terhadap input terbatas (Bounded Input) akan mencapai nilai tunak (Bounded Output) seiring waktu.\")\n",
    "else:\n",
    "    print(\"\\nKesimpulan Kestabilan: Sistem ini TIDAK STABIL atau MARGINAL STABIL.\")\n",
    "    print(\"Alasan: Setidaknya satu eigenvalue memiliki bagian real positif atau nol.\")"
   ]
