    "# SOAL NO. 5\n",
    "import control as ct\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# --- Definisi Transfer Function ---\n",
    "# H(s) = (s + 1) / (3s + 1)(4s + 1)\n",
    "# H(s) = (s + 1) / (12s^2 + 7s + 1)\n",
    "\n",
    "# Koefisien numerator (pembilang)\n",
    "numerator = [1, 1]\n",
    "\n",
    "# Koefisien denominator (penyebut)\n",
    "denominator = [12, 7, 1]\n",
    "\n",
    "# Membuat objek transfer function\n",
    "H = ct.tf(numerator, denominator)\n",
    "print(\"Transfer Function H(s):\")\n",
    "print(H)\n",
    "\n",
    "# --- a. Menentukan Poles dan Zeros ---\n",
    "poles = ct.poles(H)\n",
    "zeros = ct.zeros(H)\n",
    "\n",
    "print(\"\\nPoles sistem:\")\n",
    "print(poles)\n",
    "print(\"\\nZeros sistem:\")\n",
    "print(zeros)\n",
    "\n",
    "# --- b. Menggambar Pole-Zero Map (pzmap) ---\n",
    "plt.figure(figsize=(8, 6))\n",
    "ct.pzmap(H, plot=True, title='Pole-Zero Map H(s)')\n",
    "plt.grid(True)\n",
    "plt.show()\n",
    "\n",
    "# --- c. Interpretasi Kestabilan ---\n",
    "print(\"\\n--- Interpretasi Kestabilan ---\")\n",
    "# Kestabilan sistem LTI ditentukan oleh letak pole-pole.\n",
    "is_stable = all(pole.real < 0 for pole in poles)\n",
    "\n",
    "if is_stable:\n",
    "    print(\"Kesimpulan: Sistem ini STABIL.\")\n",
    "    print(\"Alasan: Semua pole sistem (akar dari penyebut) berada di bidang kiri (left-half plane) dari s-plane.\")\n",
    "    print(\"Bagian real dari semua pole bernilai negatif, yang berarti respons sistem akan kembali ke nilai setimbang seiring waktu.\")\n",
    "else:\n",
    "    print(\"Kesimpulan: Sistem ini TIDAK STABIL atau MARGINAL STABIL.\")\n",
    "    print(\"Alasan: Setidaknya satu pole memiliki bagian real non-negatif (yaitu nol atau positif).\")\n",
    "\n"
   ]
  },
