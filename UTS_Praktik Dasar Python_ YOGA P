   ],
   "source": [
    "# SOAL NO. 2\n",
    "import control as ct\n",
    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "\n",
    "# --- Parameter Sistem ---\n",
    "K = 3  # Gain DC tetap\n",
    "T_values = [5, 10, 30] # Variasi konstanta waktu T\n",
    "\n",
    "# --- Plotting ---\n",
    "plt.figure(figsize=(12, 8))\n",
    "plt.axhline(y=K, color='r', linestyle='--', label=f'Nilai Akhir (K = {K})')\n",
    "\n",
    "# Iterasi untuk setiap nilai T\n",
    "for T in T_values:\n",
    "    # Definisikan numerator dan denominator untuk H(s) = K / (Ts + 1)\n",
    "    numerator = [K]\n",
    "    denominator = [T, 1]\n",
    "\n",
    "    # Buat transfer function\n",
    "    H = ct.tf(numerator, denominator)\n",
    "    print(f\"Transfer Function untuk T={T}:\")\n",
    "    print(H)\n",
    "\n",
    "    # Hitung step response\n",
    "    # Tentukan rentang waktu simulasi yang cukup panjang agar semua sistem mencapai steady-state\n",
    "    # Gunakan waktu yang cukup lama, misal 5 kali T terbesar\n",
    "    max_T = max(T_values)\n",
    "    time = np.linspace(0, 5 * max_T, 500)\n",
    "    time, response = ct.step_response(H, T=time)\n",
    "\n",
    "    # Plot respons\n",
    "    plt.plot(time, response, label=f'T = {T} detik')\n",
    "\n",
    "# --- Konfigurasi Plot ---\n",
    "plt.title('Step Response Sistem Orde Pertama dengan Variasi T (K=3)')\n",
    "plt.xlabel('Waktu (detik)')\n",
    "plt.ylabel('Amplitudo Output')\n",
    "plt.grid(True)\n",
    "plt.legend()\n",
    "plt.show()\n",
    "\n",
    "# --- Interpretasi ---\n",
    "print(\"\\n--- Interpretasi Hasil Grafik ---\")\n",
    "print(f\"Nilai gain DC (K) sistem ini adalah {K}. Semua sistem akan mencapai nilai akhir (steady-state) 3 unit.\")\n",
    "print(\"Konstanta waktu (T) menentukan seberapa cepat sistem mencapai nilai akhir tersebut.\")\n",
    "print(\"Dari grafik, dapat dilihat bahwa:\")\n",
    "print(f\"- Untuk T = {T_values[0]} (terkecil), sistem mencapai nilai akhir paling cepat.\")\n",
    "print(f\"- Untuk T = {T_values[2]} (terbesar), sistem mencapai nilai akhir paling lambat.\")\n",
    "print(\"Secara umum: Semakin besar nilai konstanta waktu (T), semakin lambat respons transien sistem.\")\n"
   ]
  },
