   ],
   "source": [
    "# SOAL NO. 3\n",
    "import control as ct\n",
    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "\n",
    "# --- Parameter Time Delay dan Orde Aproksimasi ---\n",
    "tau = 2.0  # Time delay in seconds\n",
    "N = 3      # Order of the Pade approximation\n",
    "\n",
    "# --- Menggunakan Pade Approximation ---\n",
    "# ct.pade mengembalikan koefisien numerator (num) dan denominator (den)\n",
    "num_pade, den_pade = ct.pade(tau, N)\n",
    "\n",
    "# Membuat transfer function dari koefisien tersebut\n",
    "H_pade = ct.tf(num_pade, den_pade)\n",
    "\n",
    "print(f\"--- Transfer Function Pade Approximation Orde N={N} untuk tau={tau}s ---\")\n",
    "print(H_pade)\n",
    "\n",
    "# --- Menggambar Step Response ---\n",
    "\n",
    "# Buat model \"exact\" (asli) dengan time delay untuk perbandingan\n",
    "# Perlu pendekatan khusus atau penggunaan fungsi time-domain yang menangani delay secara eksplisit\n",
    "# Di sini kita fokus pada H_pade.\n",
    "\n",
    "plt.figure(figsize=(10, 6))\n",
    "\n",
    "# Waktu simulasi: pastikan cukup panjang untuk melihat respons setelah delay\n",
    "t = np.linspace(0, 4 * tau, 500)\n",
    "time, response = ct.step_response(H_pade, T=t)\n",
    "\n",
    "plt.plot(time, response, label=f'Pade Approximation Orde {N} (tau={tau}s)')\n",
    "plt.title(f'Step Response Menggunakan Pade Approximation Orde {N}')\n",
    "plt.xlabel('Waktu (detik)')\n",
    "plt.ylabel('Amplitudo Output')\n",
    "plt.grid(True)\n",
    "plt.legend()\n",
    "plt.show()\n",
    "\n",
    "# --- Kesimpulan Interpretasi ---\n",
    "print(\"\\n--- Kesimpulan Interpretasi ---\")\n",
    "print(f\"Bentuk transfer function hasil Pade approximation orde {N} adalah fungsi rasional (rasio dua polinomial).\")\n",
    "print(\"Pada grafik step response, terlihat sistem membutuhkan waktu sekitar 2 detik (tau) untuk mulai bereaksi secara signifikan terhadap input step, yang merepresentasikan penundaan waktu (time delay).\")\n",
    "print(\"Respons dari Pade approximation orde tinggi (N=3) cenderung memiliki karakteristik 'nonminimum phase' ringan di awal (terlihat sedikit overshoot atau undershoot kecil sebelum respons utama dimulai) tetapi memberikan aproksimasi yang baik terhadap perilaku frekuensi dan waktu dibandingkan orde yang lebih rendah.\")\n",
    "print(\"Pade approximation mengubah sistem penundaan waktu yang bersifat non-rasional menjadi sistem LTI rasional yang dapat dianalisis menggunakan metode kontrol standar (seperti Root Locus atau Bode Plot tradisional) yang tidak mendukung time delay eksplisit.\")\n"
   ]
  },
