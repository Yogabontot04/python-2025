   "source": [
    "# SOAL NO. 1\n",
    "import control as ct\n",
    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "\n",
    "# Koefisien numerator (pembilang) dan denominator (penyebut)\n",
    "# H(s) = 4 / (3s^2 + 2s + 1)\n",
    "numerator = [4]\n",
    "denominator = [3, 2, 1]\n",
    "\n",
    "# Membuat transfer function\n",
    "H = ct.tf(numerator, denominator)\n",
    "\n",
    "print(\"Transfer Function H(s):\")\n",
    "print(H)\n",
    "\n",
    "# Plot step response\n",
    "plt.figure(figsize=(10, 6))\n",
    "time, response = ct.step_response(H)\n",
    "plt.plot(time, response)\n",
    "plt.title('Step Response H(s) = 4 / (3s^2 + 2s + 1)')\n",
    "plt.xlabel('Waktu (detik)')\n",
    "plt.ylabel('Amplitudo')\n",
    "plt.grid(True)\n",
    "plt.show()\n",
    "\n",
    "# Analisis Kestabilan\n",
    "# Kestabilan sistem LTI ditentukan oleh letak pole-pole sistem.\n",
    "poles = ct.poles(H)\n",
    "print(\"\\nPole-pole sistem:\")\n",
    "print(poles)\n",
    "\n",
    "# Interpretasi Kestabilan\n",
    "is_stable = all(pole.real < 0 for pole in poles)\n",
    "\n",
    "if is_stable:\n",
    "    print(\"\\nInterpretasi Kestabilan: Sistem ini STABIL.\")\n",
    "    print(\"Semua pole (akar penyebut) berada di bidang kiri (left-half plane) dari s-plane, yang berarti bagian real dari setiap pole bernilai negatif.\")\n",
    "    print(\"Respons sistem terhadap input terbatas (Bounded Input) akan kembali ke keadaan setimbang (Bounded Output) seiring waktu.\")\n",
    "else:\n",
    "    print(\"\\nInterpretasi Kestabilan: Sistem ini TIDAK STABIL atau MARGINAL STABIL.\")\n",
    "    print(\"Setidaknya satu pole berada di bidang kanan (right-half plane) atau tepat di sumbu imajiner.\")\n",
    "    "
   ]
  },
