   "source": [
    "# SOAL NO. 8\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "import control as ctrl\n",
    "\n",
    "# a. Definisikan model state-space kontinu\n",
    "A = np.array([[0, 1],\n",
    "              [-1, -3]])\n",
    "B = np.array([[0],\n",
    "              [1]])\n",
    "C = np.array([[1, 0]])\n",
    "D = np.array([[0]])\n",
    "\n",
    "# Sistem kontinu\n",
    "sys_c = ct.ss(A, B, C, D)\n",
    "print(\"--- Model State-Space Kontinu ---\")\n",
    "print(sys_c)\n",
    "\n",
    "# --- 2. Discretisasi Menggunakan Metode Euler (Manual Implementation) ---\n",
    "Ts = 0.1  # Waktu sampling (Sampling time)\n",
    "\n",
    "# Hitung Ad = I + Ts * A\n",
    "Ad = np.eye(A.shape[0]) + Ts * A\n",
    "\n",
    "# Hitung Bd = Ts * B\n",
    "Bd = Ts * B\n",
    "\n",
    "# Matriks Cd dan Dd sama dengan C dan D\n",
    "Cd = C\n",
    "Dd = D\n",
    "\n",
    "sys_d_manual = ct.ss(Ad, Bd, Cd, Dd, Ts)\n",
    "\n",
    "print(f\"\\n--- Model State-Space Diskrit (Euler Method, Ts = {Ts}s) ---\")\n",
    "print(sys_d_manual)\n",
    "\n",
    "# --- 3. (Opsional) Menggunakan fungsi bawaan control.sample_system ---\n",
    "# Library control memiliki fungsi built-in untuk diskretisasi (menggunakan metode ZOH/Zero-Order Hold secara default)\n",
    "# ct.sample_system(sys_c, Ts, method='forward_euler') bisa digunakan tetapi kurang umum.\n",
    "# Implementasi manual di atas lebih transparan.\n",
    "\n",
    "# --- 4. Plot Step Response (Perbandingan) ---\n",
    "\n",
    "# Waktu simulasi untuk kedua sistem\n",
    "T_sim = np.linspace(0, 10, 1000)\n",
    "# Waktu simulasi diskrit (interval Ts)\n",
    "T_sim_d = np.arange(0, 10, Ts)\n",
    "\n",
    "# Respons Sistem Kontinu\n",
    "time_c, response_c = ct.step_response(sys_c, T=T_sim)\n",
    "\n",
    "# Respons Sistem Diskrit (ct.step_response bisa menangani sistem diskrit)\n",
    "time_d, response_d = ct.step_response(sys_d_manual, T=T_sim_d)\n",
    "\n",
    "\n",
    "plt.figure(figsize=(12, 7))\n",
    "plt.plot(time_c, response_c, 'b-', label='Sistem Kontinu Asli')\n",
    "plt.step(time_d, response_d, 'r-', where='post', label=f'Sistem Diskrit (Euler, Ts={Ts}s)')\n",
    "plt.title(f'Step Response Perbandingan Sistem Kontinu vs Diskrit (Euler, Ts={Ts}s)')\n",
    "plt.xlabel('Waktu (detik)')\n",
    "plt.ylabel('Output y(t)')\n",
    "plt.grid(True)\n",
    "plt.legend()\n",
    "plt.show()\n",
    "\n",
    "\n",
    "# --- 5. Interpretasi Kestabilan Sistem Diskrit ---\n",
    "# Kestabilan sistem diskrit ditentukan oleh letak eigenvalues (poles) relative terhadap lingkaran unit (unit circle)\n",
    "# Poles harus berada di dalam unit circle (|z| < 1)\n",
    "\n",
    "eigenvalues_d = np.linalg.eigvals(Ad)\n",
    "print(\"\\n--- Interpretasi Kestabilan Sistem Diskrit ---\")\n",
    "print(f\"Eigenvalues Matriks Ad (Poles Sistem Diskrit): {eigenvalues_d}\")\n",
    "\n",
    "# Cek kestabilan: semua poles harus berada di dalam lingkaran unit\n",
    "is_stable_d = all(abs(eig) < 1 for eig in eigenvalues_d)\n",
    "\n",
    "if is_stable_d:\n",
    "    print(\"\\nKesimpulan Kestabilan: Sistem diskrit STABIL.\")\n",
    "    print(\"Alasan: Semua poles sistem diskrit berada di dalam lingkaran unit (|z| < 1) pada z-plane.\")\n",
    "    print(\"Metode Euler berhasil mempertahankan kestabilan sistem kontinu asli (yang stabil).\")\n",
    "else:\n",
    "    print(\"\\nKesimpulan Kestabilan: Sistem diskrit TIDAK STABIL.\")\n",
    "    print(\"Alasan: Setidaknya satu pole berada di luar atau tepat pada lingkaran unit.\")\n",
    "    print(\"Catatan: Metode Euler dapat menyebabkan ketidakstabilan jika waktu sampling Ts terlalu besar, meskipun sistem aslinya stabil.\")"
   ]
  },
