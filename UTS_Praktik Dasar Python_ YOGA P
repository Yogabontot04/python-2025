   "source": [
    "# SOAL NO. 9\n",
    "import control as ct\n",
    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "\n",
    "# Definisi Matriks State-Space untuk Sistem SISO\n",
    "\n",
    "A = np.array([[0, 1],\n",
    "              [-2, -3]])\n",
    "B = np.array([[0],\n",
    "              [1]])\n",
    "C = np.array([[1, 0]])\n",
    "D = np.array([[0]])\n",
    "\n",
    "# Membuat Objek Sistem StateSpace\n",
    "\n",
    "# Menggunakan fungsi ct.ss() untuk membuat objek sistem\n",
    "sys = ct.ss(A, B, C, D)\n",
    "\n",
    "print(\"--- Model Sistem SISO State-Space ---\")\n",
    "print(sys)\n",
    "\n",
    "print(\"\\nDetail Matriks:\")\n",
    "print(f\"Matriks A:\\n{A}\")\n",
    "print(f\"Matriks B:\\n{B}\")\n",
    "print(f\"Matriks C:\\n{C}\")\n",
    "print(f\"Matriks D:\\n{D}\")\n",
    "\n",
    "\n",
    "# Simulasi dan Plotting Step Response (Opsional, untuk visualisasi)\n",
    "\n",
    "plt.figure(figsize=(10, 6))\n",
    "\n",
    "# Tentukan rentang waktu simulasi\n",
    "# Sistem ini stabil, jadi 5-10 detik sudah cukup\n",
    "time = np.linspace(0, 8, 500)\n",
    "\n",
    "# Hitung step response\n",
    "# response akan berisi nilai output y(t)\n",
    "time, response = ct.step_response(sys, T=time)\n",
    "\n",
    "plt.plot(time, response)\n",
    "plt.title('Step Response Sistem SISO State-Space')\n",
    "plt.xlabel('Waktu (detik)')\n",
    "plt.ylabel('Output y(t)')\n",
    "plt.grid(True)\n",
    "plt.show()\n",
    "poles = ct.poles(sys_tf)\n",
    "print(f\"\\nPoles sistem: {poles}\")\n",
    "if all(pole.real < 0 for pole in poles):\n",
    "    print(\"Interpretasi: Sistem ini STABIL karena semua poles memiliki bagian real negatif.\")"
   ]
